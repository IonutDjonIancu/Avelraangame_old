@{
    ViewData["Title"] = "Home Page";
}
<br />

<div class="text-left jumbotron" style="opacity: 0.7; background-color:black; color:navajowhite">
    <div id="nonLoggedDiv" class="mb-3" style="display:none">
        <select id="symbolWard" class="form-select col-lg-3" aria-label="symbol selector">
            <option selected>Select symbol</option>
            <option value="Epsilon">Epsilon &epsilon;</option>
            <option value="Omicron">Omicron &omicron;</option>
            <option value="Sigma">Sigma &sigma;</option>
        </select>
        <input type="password" class="form-control col-lg-3 text-center" id="powerWardInput" placeholder="enter power ward">
        <br />
        <button id="logonBtn" class="btn btn-outline-success col-lg-3">Submit</button>
        <br />
        <br />
        <button id="createPlayerBtn" class="btn btn-outline-dark col-lg-3">I don't have one</button>
    </div>
    <div id="loggedDiv" style="display:none">
    </div>
</div>





<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script src="~/js/home/index.js"></script>
