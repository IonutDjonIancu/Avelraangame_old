@{
    ViewData["Title"] = "CreateCharacter";
}
<br />
<p style="color:navajowhite">Create character</p>

@{
    var charId = ViewBag.characterId;
    var statsRoll = ViewBag.statsRoll;
}


<div class="text-left jumbotron" style="opacity:0.7; background-color:black; color:navajowhite">
    <p style="color:navajowhite">Create character</p>
    <p>Character id: @charId</p>
    <p>Stats points: @statsRoll</p>
</div>


<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script src="~/js/characterIndex.js"></script>